{
  "expo": {
    "name": "WeBarrio",
    "description": "Vuelve a tu barrio.",
    "slug": "webarrio-mobile",
    "privacy": "unlisted",
    "sdkVersion": "25.0.0",
    "platforms": ["ios", "android"],
    "version": "0.2.0",
    "orientation": "portrait",
    "icon": "./assets/images/app_icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "cl.webarrio.app"
    },
    "android": {
      "package": "cl.webarrio.app"
    },
    "notification": {
      "icon": "./assets/images/notification_icon.png",
      "color": "#000000",
      "androidCollapsedTitle": "#{unread_notifications} nuevas notificaciones"
    }
  }
}
